{
    "name": "express-typescript-todo",
    "version": "1.0.0",
    "scripts": {
        "install-root": "npm install",
        "install-frontend": "cd frontend && npm install",
        "install-backend": "cd backend && npm install",
        "install-all": "concurrently 'npm:install-root' 'npm:install-frontend' 'npm:install-backend'",
        "run-backend-dev": "ts-node-dev backend/src/index",
        "build-backend": "tsc -p ./backend",
        "run-backend": "node ./backend/dist/index",
        "run-frontend-dev": "cd frontend && npm run start",
        "build-frontend": "cd frontend && npm run build",
        "run-all-dev": "concurrently 'npm:run-frontend-dev' 'npm:run-backend-dev'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jordan-git/typescript-mern-todo-app.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jordan-git/typescript-mern-todo-app/issues"
    },
    "homepage": "https://github.com/jordan-git/typescript-mern-todo-app#readme",
    "devDependencies": {
        "concurrently": "^5.3.0",
        "ts-node": "^9.0.0",
        "ts-node-dev": "^1.0.0-pre.62",
        "tsc": "^1.20150623.0",
        "typescript": "^4.0.2"
    }
}
